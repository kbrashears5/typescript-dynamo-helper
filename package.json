{
    "name": "typescript-aws-dynamo-helper",
    "version": "1.0.0",
    "description": "Typescript AWS DynamoDB Helper",
    "repository": {
        "type": "git",
        "url": "https://github.com/kbrashears5/typescript-aws-dynamo-helper.git"
    },
    "bugs": {
        "url": "https://github.com/kbrashears5/typescript-aws-dynamo-helper/issues"
    },
    "homepage": "https://github.com/kbrashears5/typescript-aws-dynamo-helper#readme",
    "author": "Kevin Brashears",
    "keywords": [
        "javascript",
        "typescript",
        "aws",
        "dynamo",
        "helper",
        "lambda"
    ],
    "main": "lib/index.js",
