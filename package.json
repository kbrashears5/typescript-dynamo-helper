        "typescript-ilogger": "1.0.13"
    },
    "devDependencies": {
        "@types/aws-lambda": "8.10.59",
        "@types/jest": "26.0.8",
        "@types/node": "14.0.27",
        "aws-sdk": "2.737.0",
        "jest": "26.2.1",
        "jest-junit": "11.1.0",
        "rimraf": "3.0.2",
        "ts-jest": "26.1.4",
        "tslint": "6.1.2",
        "typescript": "3.9.7"
    },
    "files": [
        "lib/**"
    ],
    "jest": {
        "preset": "ts-jest",
